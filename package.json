{
  "name": "gatsby-plugin-workbox",
  "description": "Gatsby plugin which sets up a site to be able to run offline",
  "version": "0.0.1",
  "author": "Niels Bik <hey@nielsbik.nl> (https://nielsbik.nl)",
  "bugs": "https://github.com/nielsrowinbik/gatsby-plugin-workbox/issues",
  "dependencies": {
    "babel-runtime": "^6.26.0",
    "workbox-sw": "^3.2.0"
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "cross-env": "^5.0.5"
  },
  "homepage": "https://github.com/nielsrowinbik/gatsby-plugin-workbox#readme",
  "keywords": [
    "gatsby",
    "gatsby-plugin",
    "workbox",
    "precache",
    "service-worker"
  ],
  "license": "MIT",
  "main": "index.js",
  "peerDependencies": {
    "gatsby": "^1.0.0"
  },
  "repository": "nielsrowinbik/gatsby-plugin-workbox",
  "scripts": {
    "build": "babel src --out-dir . --ignore __tests__",
    "prepublish": "cross-env NODE_ENV=production npm run build",
    "watch": "babel -w src --out-dir . --ignore __tests__"
